
<!-- 동적 페이지 
	자바 
	Date() 객체를 이용해서 날짜 반환 => java.util.*
	
	SimpleDateFormat("yyyy-MM-dd") => java.text.*
	: Date() 날짜를 원하는 형태로 반환
-->
<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<%@ page import="java.util.*, java.text.*" %>

<!DOCTYPE html>
<html>
<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
</head>
<body>
<% Date date = new Date();
	
%>
</body>
</html>